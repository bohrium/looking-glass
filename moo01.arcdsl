(split<int><gridpair> (many noise) \x1:int ->
(split<shape><gridpair> (gen_shape noise x1) \x2:shape ->
(split<ptdgrid><gridpair> (reserve_shape
   noise
   (monochrome x2 cyan)
   x2
   (center x2)) \x3:ptdgrid ->
(pair<gridpair>
   (paint_sprite
      x3
      (monochrome
         x2
         (cond<color> x1 cyan cyan))
      (center x2))
   (paint_sprite
      x3
      (monochrome
         x2
         (cond<color> x1 cyan cyan))
      (center x2))))))